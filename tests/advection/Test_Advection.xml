<?xml version="1.0" encoding="utf-8"?>
<ARTSS>
  <physical_parameters>
    <t_end>2.0</t_end>
    <dt>0.001</dt>
  </physical_parameters>
  <solver description="AdvectionSolver">
    <advection type="SemiLagrangian" field="u,v,w"></advection>
    <solution available="Yes">
      <tol>1e-03</tol>
    </solution>
  </solver>
  <domain_parameters>
    <X1>0.</X1>
    <X2>2.0</X2>
    <Y1>0.</Y1>
    <Y2>2.0</Y2>
    <Z1>0.</Z1>
    <Z2>1.0</Z2>
    <x1>0.</x1>
    <x2>2.0</x2>
    <y1>0.</y1>
    <y2>2.0</y2>
    <z1>0.</z1>
    <z2>1.0</z2>
    <nx>40</nx>
    <ny>40</ny>
    <nz>1</nz>
  </domain_parameters>
  <adaption dynamic="No" data_extraction="No"></adaption>
  <boundaries>
    <boundary field="u,v,w"
    patch="front,back,left,right,bottom,top" type="dirichlet"
    value="0.0" />
  </boundaries>
  <obstacles enabled="No" />
  <surfaces enabled="No" />
  <initial_conditions usr_fct="GaussBubble" random="No">
    <u_lin>0.5</u_lin>
    <v_lin>0.5</v_lin>
    <w_lin>0.25</w_lin>
    <xshift>1.025</xshift>
    <yshift>1.025</yshift>
    <zshift>0.5</zshift>
    <l>0.03125</l>
  </initial_conditions>
  <visualisation save_vtk="Yes" save_csv="No">
    <vtk_nth_plot>200</vtk_nth_plot>
  </visualisation>
  <logging>
    <level>INFO</level>
    <file>output_test_advection.log</file>
  </logging>
</ARTSS>
