<?xml version="1.0" encoding="utf-8"?>
<ARTSS>
  <physical_parameters>
    <t_end>0.1</t_end>
    <dt>0.1</dt>
  </physical_parameters>
  <solver description="PressureSolver">
    <pressure type="VCycleMG" field="p">
      <n_level>5</n_level>
      <n_cycle>2</n_cycle>
      <max_cycle>100</max_cycle>
      <tol_res>1e-07</tol_res>
      <diffusion type="Jacobi" field="p">
        <n_relax>4</n_relax>
        <max_solve>100</max_solve>
        <tol_res>1e-07</tol_res>
        <w>0.6666666667</w>
      </diffusion>
    </pressure>
    <solution available="Yes">
      <tol>1e-03</tol>
    </solution>
  </solver>
  <domain_parameters>
    <X1>0.</X1>
    <X2>2.</X2>
    <Y1>0.</Y1>
    <Y2>2.</Y2>
    <Z1>0.</Z1>
    <Z2>2.</Z2>
    <x1>0.</x1>
    <x2>2.</x2>
    <y1>0.</y1>
    <y2>2.</y2>
    <z1>0.</z1>
    <z2>2.</z2>
    <nx>64</nx>
    <ny>64</ny>
    <nz>64</nz>
  </domain_parameters>
  <adaption dynamic="No" data_extraction="No"></adaption>
  <boundaries>
    <boundary field="p" patch="front,back,left,right,bottom,top"
    type="dirichlet" value="0.0" />
  </boundaries>
  <obstacles enabled="No" />
  <surfaces enabled="No" />
  <initial_conditions usr_fct="SinSinSin" random="No">
    <l>2.</l>
  </initial_conditions>
  <visualisation save_vtk="Yes" save_csv="No">
    <vtk_nth_plot>10</vtk_nth_plot>
  </visualisation>
  <logging>
    <level>INFO</level>
    <file>output_test_pressure.log</file>
  </logging>
</ARTSS>
